<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>

<script>
  // Fungsi untuk menghitung impuls
  function hitungImpuls() {
  const F = parseFloat(document.getElementById('gaya').value) || 0; // Gaya
  const deltaT = parseFloat(document.getElementById('deltaT').value) || 1; // Waktu
  const I = F * deltaT; // I = F × Δt
  document.getElementById('hasilImpuls').textContent = I.toFixed(2);
}
</script>

</head>
<body>
  <h2>Hitung Impuls</h2>
    <label for="gaya">Masukkan gaya (N):</label>
    <input type="number" id="gaya" />
    <label for="deltaT">Masukkan Δt (s):</label>
    <input type="number" id="deltaT" />
    <button onclick="hitungImpuls()">Hitung Impuls</button>
    <p>Hasil: <span id="hasilImpuls"> </span></p>
</body>
</html>